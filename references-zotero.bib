	keywords = {Computer Science, general, Cyberinfrastructure, Interdisciplinary Studies, Psychology, general, Social Sciences, general, User Interfaces and Human Computer Interaction, data curation, data practices, eScience, environmental sciences, scientific collaboration, scientific software development, sensor networks, technology research},
	pages = {485--523}
}

