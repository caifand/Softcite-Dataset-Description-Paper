	journal = {Concurrency and Computation: Practice and Experience},
	author = {Furlani, Thomas R. and Jones, Matthew D. and Gallo, Steven M. and Bruno, Andrew E. and Lu, Charng-Da and Ghadersohi, Amin and Gentner, Ryan J. and Patra, Abani and DeLeon, Robert L. and von Laszewski, Gregor and Wang, Fugang and Zimmerman, Ann},
