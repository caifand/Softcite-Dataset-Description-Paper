@article{blumenthal_data_2006,
	title = {Data withholding in genetics and the other life sciences: prevalences and predictors},
	volume = {81},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/16436574},
	abstract = {PURPOSE: To better understand the variety and prevalence of data withholding in genetics and the other life sciences and to explore factors associated with these behaviors. METHOD: In 2000, a sample of 2,893 geneticists and other life scientists (OLS) at the 100 most research-intensive universities in the United States were surveyed concerning data withholding and sharing. The instrument was developed and pretested in 1999. The two primary outcome measures were withholding in verbal exchanges with colleagues about unpublished research (verbal withholding) and withholding as part of the publishing process (publishing withholding). The independent variables related to the personal characteristics, research characteristics of faculty, and previous experience with data withholding. RESULTS: A total of 1,849 faculty responded (64{\textbackslash}\%): 1,240 geneticists and 600 OLS. Forty-four percent of geneticists and 32{\textbackslash}\% of OLS reported participating in any one of 13 forms of data withholding in the three previous years. Publishing withholding (geneticists 35{\textbackslash}\%, OLS 25{\textbackslash}\%) was more frequent than verbal withholding (geneticists 23{\textbackslash}\%, OLS 12{\textbackslash}\%). In multivariate analyses, male gender, participation in relationships with industry, mentors' discouraging data sharing, receipt of formal instruction in data sharing, and negative past experience with sharing were significantly associated with either verbal or publishing withholding among either geneticists or OLS. CONCLUSIONS: Data withholding is common in biomedical science, takes multiple forms, is influenced by a variety of characteristics of investigators and their training, and varies by field of science. Encouraging openness during the formative experiences of young investigators may be critical to increased data sharing, but the effects of formal training do not appear straightforward.},
	number = {2},
	journal = {Academic Medicine},
	author = {Blumenthal, D. and Campbell, E. and Gokhale, M. and Yucel, R. and Clarridge, B. and Hilgartner, S. and Holtzman, N.},
	year = {2006},
	keywords = {data-sharing, data-withholding, motivation},
	pages = {--145}
}

@article{vogeli_data_2006,
	title = {Data withholding and the next generation of scientists: results of a national survey},
	volume = {81},
	url = {http://view.ncbi.nlm.nih.gov/pubmed/16436573},
	abstract = {PURPOSE: To provide the first national data on the nature, extent, and consequences of withholding among life science trainees. METHOD: In 2003, the authors surveyed 1,077 second-year doctoral students and postdoctoral fellows in life sciences at 50 U.S. universities, with a comparison group of trainees in computer science and chemical engineering. The study variables examined trainees' exposure to and the consequences of data withholding. RESULTS: Two hundred forty-six trainees (23.0{\textbackslash}\%) reported that they had asked for and been denied access to information, data, materials, or programming associated with published research and 221 (20.6{\textbackslash}\%) to unpublished research. Eighty-five trainees (7.9{\textbackslash}\%) reported that they had denied another academic scientist's request(s) related to their own published research. Five hundred thirty-three trainees (50.8{\textbackslash}\%) reported that withholding had had a negative effect on the progress of their research, 508 (48.5{\textbackslash}\%) on the rate of discovery in their lab/research group, 472 (45.0{\textbackslash}\%) on the quality of their relationships with academic scientists, 346 (33.0{\textbackslash}\%) on the quality of their education, and 299 (28.5{\textbackslash}\%) on the level of communication in their lab/research group. Trainees denied access to research were significantly more likely to report that data withholding had had a negative effect on several aspects of the educational experience. CONCLUSIONS: Data withholding had demonstrated negative effects on trainees. The life sciences, more so than chemical engineering or computer science, will have to address this issue among its trainees. Failure to do so could result in delayed research, inefficient training, and a culture of withholding among future life scientists.},
	number = {2},
	journal = {Academic Medicine},
	author = {Vogeli, C. and Yucel, R. and Bendavid, E. and Jones, L. and Anderson, M. and Louis, K. and Campbell, E.},
	year = {2006},
	keywords = {all, data-sharing, data-withholding, file-import-08-12-31, file-import-09-01-05, motivation, psb-talk, psb2009-openscience},
	pages = {--136}
}

