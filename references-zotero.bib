	journal = {Organization Science},
	author = {von Hippel, Eric and von Krogh, Georg},
	year = {2003},
	keywords = {COLLECTIVE action, COMPUTER software, INVESTMENTS, Incentives, Innovation, Intellectual property, Open source software, User Innovation, Users},
